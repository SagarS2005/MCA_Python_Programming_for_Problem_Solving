   "source": [
    "num=int(input(\"Enter Number to find factorial:\"))\n",
    "print(\"Enter Number to find factorial:\",num)\n",
    "\n",
    "def rec(num):\n",
    "    if num==0:\n",
    "        return 0\n",
    "    elif num==1:\n",
    "        return 1\n",
    "    else:\n",
    "        return num*rec(num-1)\n",
    "\n",
    "print(\"Factorial of num\",num,\" is :\",rec(num))"
   ]
  }
