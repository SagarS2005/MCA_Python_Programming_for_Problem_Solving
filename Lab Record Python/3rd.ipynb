   "source": [
    "num=int(input(\"Enter number : \"))\n",
    "lst=list(int (digit) for digit in str(num))\n",
    "print(\"Original Number :\",lst)\n",
    "length=len(lst)\n",
    "# print(length)\n",
    "lst2=['']*length\n",
    "\n",
    "for j in range(0,len(lst)):\n",
    "    lst2[length-1]=lst[j]\n",
    "    length=length-1\n",
    "\n",
    "print(\"Reversed Number:\",lst2)"
   ]
